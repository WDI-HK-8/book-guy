app.controller('MyRequestsCtrl', ['$scope', '$http', '$location', '$auth', '$filter', function($scope, $http, $location, $auth, $filter){

  var url = "<%= ENV['URL'] %>" || "http://localhost:3000";

  $scope.outgoing = []

  $http.get(url + '/requests').success(function(response){
    $scope.dataOutgoings = response.outgoings
    $scope.dataIncomings = response.incomings
    $scope.outgoings = [].concat($scope.dataOutgoings);
    $scope.incomings = [].concat($scope.dataIncomings);

    // console.log($scope.outgoings)

    console.log($scope.incomings)
  });



}]);
